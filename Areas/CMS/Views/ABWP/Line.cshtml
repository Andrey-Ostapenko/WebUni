@{
    Layout = null;
}
@model long

<div class="catalog-tab-content" id="catalog-tab-data">
    @{ Html.RenderPartial(MVC.CMS.WebUni.Views.CatalogTabDataCommon, Model);}
    <div id="catalog-tab-data-int">
        @{ Html.RenderAction(MVC.CMS.WebUni.LineData(Model)); }
    </div>
</div>

<div class="catalog-tab-content" id="catalog-tab-shop">
    @{ Html.RenderAction(MVC.CMS.WebUni.LineShop(Model)); }
</div>



<div class="catalog-tab-content" id="catalog-tab-design">
    @{ Html.RenderPartial(MVC.CMS.WebUni.Views.CatalogTabDesignCommon, Model);}
    <div id="catalog-tab-design-int">
        @{ Html.RenderAction(MVC.CMS.WebUni.LineDesign(Model)); }
    </div>
</div>
